<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="car2">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import car2 urdf file -->
    <xacro:include filename="$(find car2)/urdf/car2.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="car2.ros2_control.xacro" />


    <xacro:car2_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
